    "pol = 0\n",
    "prom = 0\n",
    "maxpol = 0\n",
    "for i in range (100, 1000):\n",
    "    for j in range (100, 1000):\n",
    "        prom = str(i * j)\n",
    "        if prom == prom[::-1]:\n",
    "            pol = prom\n",
    "            pol = int(pol)\n",
    "            if pol > maxpol:\n",
    "                maxpol = pol\n",
    "print(maxpol)"
